
SUBDIRS =	\
./test/tcpserver/	\
./test/tcpclient/	\

define fun1
	echo "Making $(1) in $(SUBDIRS)" 
	for subdir in $(SUBDIRS); do \
		(cd $$subdir && make $(1)); \
	done;
endef

all:
	$(call fun1,all)

clean:
	rm -rf *.out
	rm -rf *.exe
	$(call fun1,clean)
cleanall:
	rm -rf *.out
	rm -rf *.exe
	rm -rf *.so*
	$(call fun1,cleanall)
